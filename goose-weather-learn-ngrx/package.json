{
  "name": "goose-weather",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-environment": "cp src/environments/local_environment.ts src/environments/environment.ts",
    "build-local": "npm run copy-environment && npm run build",
    "serve-local": "npm run copy-environment && ng serve",
    "coverage-report": "ng test --code-coverage --watch=true",
    "delete-coverage": "rm -rf coverage",
    "code-coverage": "npm run delete-coverage && npm run coverage-report",
    "build-prod": "ng build --prod",
    "test-prod": "ng test --watch=false",
    "firebase-deploy": "firebase deploy",
    "environment-variables": "python scripts/environment_variables.py $OPEN_WEATHER_MAP_API_KEY"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@ngxd/core": "^10.0.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "puppeteer": "^1.12.2",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.2.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha": "^2.0.1",
    "mocha": "^8.2.1",
    "protractor": "^7.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^6.0.0",
    "typescript": "^4.0.3"
  }
}
